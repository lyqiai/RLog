<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunlu.log.dao.LogDao">
    <insert id="addLog" parameterType="com.yunlu.log.domain.Log" useGeneratedKeys="true" keyProperty="id">
        insert into t_log(thread_name, content)
        values(#{threadName},#{content})
    </insert>
</mapper>
