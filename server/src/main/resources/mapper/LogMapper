<mapper namespace="com.yunlu.log.mapper.ILogMapper">
    <insert id="addLog" parameterType="com.yunlu.log.entry.Log">
        insert into t_log(`level`, thread_name, content, `time`) values(#{level},#{threadName},#{content},#{time})
    </insert>
</mapper>
